{
  "_id": "fielmann",
  "startUrl": [
    "https://www.fielmann.de/brillen/"
  ],
  "selectors": [
    {
      "id": "product",
      "multiple": true,
      "parentSelectors": [
        "next"
      ],
      "selector": "main div[data-testid=\"list_product\"]",
      "type": "SelectorElement"
    },
    {
      "id": "next",
      "paginationType": "clickMore",
      "parentSelectors": [
        "_root",
        "next"
      ],
      "selector": "main button[data-testid='pagination-next-button']",
      "type": "SelectorPagination"
    },
    {
      "id": "anchor",
      "linkType": "linkFromHref",
      "multiple": false,
      "parentSelectors": [
        "product"
      ],
      "selector": ".p-4 a",
      "type": "SelectorLink"
    },
    {
      "id": "marke",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(1) li:nth-of-type(1) > span",
      "type": "SelectorText"
    },
    {
      "id": "modell",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(1) li:nth-of-type(2) > span",
      "type": "SelectorText"
    },
    {
      "id": "herstellerfarbcode",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(1) li:nth-of-type(3) > span",
      "type": "SelectorText"
    },
    {
      "id": "zielgruppe",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(1) li:nth-of-type(4) > span",
      "type": "SelectorText"
    },
    {
      "id": "brillenform",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(2) li:nth-of-type(1) > span",
      "type": "SelectorText"
    },
    {
      "id": "fassungstyp",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(2) li:nth-of-type(2) > span",
      "type": "SelectorText"
    },
    {
      "id": "stegtyp",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(2) li:nth-of-type(3) > span",
      "type": "SelectorText"
    },
    {
      "id": "scharniertyp",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(2) li:nth-of-type(4) > span",
      "type": "SelectorText"
    },
    {
      "id": "fassungsfarbe",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(3) li:nth-of-type(1) > span",
      "type": "SelectorText"
    },
    {
      "id": "artikelnummer",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div.attribute-table ul:nth-of-type(3) li:nth-of-type(2) > span",
      "type": "SelectorText"
    },
    {
      "id": "price",
      "multiple": false,
      "parentSelectors": [
        "anchor"
      ],
      "regex": "",
      "selector": "div[data-testid=\"product-price\"] span:nth-of-type(2)",
      "type": "SelectorText"
    },
    {
      "extractAttribute": "src",
      "id": "images",
      "parentSelectors": [
        "anchor"
      ],
      "selector": "div.gallery div[tabindex=\"0\"] img",
      "type": "SelectorGroup"
    }
  ]
}
